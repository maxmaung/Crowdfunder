<!DOCTYPE html>
<html>
  <head>
    <title>Crowdfunder</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <span class = "userinfo">
            <span id = "log-in-button"><%= link_to "Log In", new_session_path %></span> |
            <span id = "sign-up-button"><%= link_to "Sign Up", new_user_path %></span>
      </span>
    <%= yield %>
  </body>
</html>
