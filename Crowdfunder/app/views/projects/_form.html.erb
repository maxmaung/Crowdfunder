<%= form_for @project do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>


  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div class="field">
    <%= f.label :start_date %>
    <%= f.datetime_field :start_date %>
  </div>

  <div class="field">
    <%= f.label :end_date %>
    <%= f.datetime_field :end_date %>
  </div>


  <div class="field">
    <%= f.label :goal%>
    <%= f.number_field :goal %>
  </div>



<%= f.submit %>
<% end %>
